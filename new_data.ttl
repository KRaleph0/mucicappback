@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix komc: <https://knowledgemap.kr/komc/def/> .
@prefix dct: <http://purl.org/dc/terms/> .

komc:WeatherContext a skos:ConceptScheme ;
    dct:title "Weather Contexts for Music Recommendation"@en ;
    skos:hasTopConcept komc:Weather_Rain, komc:Weather_Clear, komc:Weather_Clouds, komc:Weather_Snow, komc:Weather_Thunderstorm, komc:Weather_Drizzle, komc:Weather_Default .

komc:Weather_Rain a skos:Concept ; skos:prefLabel "Rain"@en ; skos:related komc:tag_RainyDay, komc:tag_Sentimental, komc:tag_Depressed, komc:tag_Calm .
komc:Weather_Clear a skos:Concept ; skos:prefLabel "Clear"@en ; skos:related komc:tag_Drive, komc:tag_Excited, komc:tag_Refreshing, komc:tag_Travel .
komc:Weather_Clouds a skos:Concept ; skos:prefLabel "Clouds"@en ; skos:related komc:tag_Walk, komc:tag_Calm, komc:tag_Cafe, komc:tag_Rest .
komc:Weather_Snow a skos:Concept ; skos:prefLabel "Snow"@en ; skos:related komc:tag_Winter, komc:tag_Snow, komc:tag_Warm, komc:tag_Christmas .
komc:Weather_Thunderstorm a skos:Concept ; skos:prefLabel "Thunderstorm"@en ; skos:related komc:tag_Intense, komc:tag_Tension, komc:tag_Dark .
komc:Weather_Drizzle a skos:Concept ; skos:prefLabel "Drizzle"@en ; skos:related komc:tag_Rain, komc:tag_Dawn, komc:tag_Sentimental .
komc:Weather_Default a skos:Concept ; skos:prefLabel "Default"@en ; skos:related komc:tag_Refresh, komc:tag_Rest, komc:tag_Trendy .

komc:GenreContext a skos:ConceptScheme ;
    dct:title "Genre Concepts with Sub-genres"@en ;
    skos:hasTopConcept komc:Genre_Pop, komc:Genre_Ballad, komc:Genre_Dance, komc:Genre_HipHop, komc:Genre_RnB, komc:Genre_Rock, komc:Genre_Indie .

komc:Genre_Pop a skos:Concept ; 
    skos:prefLabel "Pop"@en ; 
    skos:narrower komc:Genre_KPop, komc:Genre_JPop, komc:Genre_SynthPop ; 
    skos:related komc:tag_Pop, komc:tag_Trendy, komc:tag_Popular .

komc:Genre_KPop a skos:Concept ;
    skos:prefLabel "K-Pop"@en ;
    skos:broader komc:Genre_Pop ;
    skos:related komc:tag_Korea, komc:tag_Idol, komc:tag_Performance .

komc:Genre_JPop a skos:Concept ; 
    skos:prefLabel "J-Pop"@en ; 
    skos:broader komc:Genre_Pop ;
    skos:narrower komc:Genre_CityPop, komc:Genre_AniSong ;
    skos:related komc:tag_Jpop, komc:tag_Japan .

komc:Genre_SynthPop a skos:Concept ;
    skos:prefLabel "Synth Pop"@en ;
    skos:broader komc:Genre_Pop ;
    skos:related komc:tag_Retro, komc:tag_Synthesizer, komc:tag_NewWave .

komc:Genre_CityPop a skos:Concept ;
    skos:prefLabel "City Pop"@en ;
    skos:broader komc:Genre_JPop ;
    skos:related komc:tag_Retro, komc:tag_Drive, komc:tag_Night, komc:tag_City .

komc:Genre_AniSong a skos:Concept ;
    skos:prefLabel "Animation Song"@en ;
    skos:broader komc:Genre_JPop ;
    skos:related komc:tag_Anime, komc:tag_Excited, komc:tag_Otaku .

komc:Genre_HipHop a skos:Concept ; 
    skos:prefLabel "HipHop"@en ; 
    skos:narrower komc:Genre_Trap, komc:Genre_Boombap ;
    skos:related komc:tag_HipHop, komc:tag_Rap, komc:tag_Swag .

komc:Genre_Trap a skos:Concept ;
    skos:prefLabel "Trap"@en ;
    skos:broader komc:Genre_HipHop ;
    skos:related komc:tag_Beat, komc:tag_Bass, komc:tag_Club .

komc:Genre_Boombap a skos:Concept ;
    skos:prefLabel "Boom Bap"@en ;
    skos:broader komc:Genre_HipHop ;
    skos:related komc:tag_OldSchool, komc:tag_Groovy .

komc:Genre_Rock a skos:Concept ; 
    skos:prefLabel "Rock"@en ; 
    skos:narrower komc:Genre_Metal, komc:Genre_Alternative ;
    skos:related komc:tag_Rock, komc:tag_Band, komc:tag_Cool .

komc:Genre_Metal a skos:Concept ;
    skos:prefLabel "Metal"@en ;
    skos:broader komc:Genre_Rock ;
    skos:related komc:tag_Intense, komc:tag_Dark, komc:tag_ElectricGuitar .

komc:Genre_Alternative a skos:Concept ;
    skos:prefLabel "Alternative Rock"@en ;
    skos:broader komc:Genre_Rock ;
    skos:related komc:tag_Sentimental, komc:tag_ModernRock .

komc:Genre_RnB a skos:Concept ; 
    skos:prefLabel "R&B"@en ; 
    skos:narrower komc:Genre_NeoSoul ;
    skos:related komc:tag_RnB, komc:tag_Soul, komc:tag_Groovy .

komc:Genre_NeoSoul a skos:Concept ;
    skos:prefLabel "Neo Soul"@en ;
    skos:broader komc:Genre_RnB ;
    skos:related komc:tag_Jazz, komc:tag_Calm, komc:tag_Night .

komc:Genre_Ballad a skos:Concept ; skos:prefLabel "Ballad"@en ; skos:related komc:tag_Ballad, komc:tag_Sentimental, komc:tag_Sad, komc:tag_Love .
komc:Genre_Dance a skos:Concept ; skos:prefLabel "Dance"@en ; skos:narrower komc:Genre_House ; skos:related komc:tag_Dance, komc:tag_Party, komc:tag_Excited .
komc:Genre_House a skos:Concept ; skos:prefLabel "House"@en ; skos:broader komc:Genre_Dance ; skos:related komc:tag_Club, komc:tag_Beat .
komc:Genre_Indie a skos:Concept ; skos:prefLabel "Indie"@en ; skos:related komc:tag_Indie, komc:tag_Acoustic, komc:tag_Cafe .

